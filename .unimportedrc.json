{
	"entry": [
		"apps/server-*/index.ts",
		"apps/tools/src/index.tsx",
		"apps/web-*/src/index.tsx",
		"modules/@creature-chess/*/index.ts",
		"modules/@shoki/*/index.ts"
	],
	"extensions": [".tsx", ".ts"],
	"ignorePatterns": [
		"**/node_modules/**",
		"**/*.stories.tsx",
		"**/*.test.{ts,tsx}"
	],
	"ignoreUnresolved": [
		"@redux-saga/types",
		"body-parser",
		"csstype",
		"delay",
		"discord.js",
		"express",
		"react-dnd-multi-backend/dist/cjs/HTML5toTouch",
		"react-dnd-multi-backend/dist/esm/HTML5toTouch",
		"socket.io/dist/typed-events",
		"winston"
	],
	"ignoreUnused": ["@types/react", "@types/react-dom", "ts-node-dev"],
	"ignoreUnimported": [
		"apps/server-game/index.ts",
		"apps/server-info/index.ts",
		"apps/tools/src/index.tsx",
		"apps/web-game/src/index.tsx",
		"apps/web-menu/src/index.tsx",
		"modules/@creature-chess/auth-server/index.ts",
		"modules/@creature-chess/auth-web/index.ts",
		"modules/@creature-chess/battle/index.ts",
		"modules/@creature-chess/bot/index.ts",
		"modules/@creature-chess/data/index.ts",
		"modules/@creature-chess/gamemode/index.ts",
		"modules/@creature-chess/models/index.ts",
		"modules/@creature-chess/networking/index.ts",
		"modules/@creature-chess/ui/index.ts",
		"modules/@shoki/board-react/index.ts",
		"modules/@shoki/board/index.ts",
		"modules/@shoki/engine/index.ts",
		"modules/@shoki/networking/index.ts"
	]
}
